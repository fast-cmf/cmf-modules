{
    "name": "fast-cmf/cmf-modules",
    "description": "The modules file",
    "keywords": ["laravel", "modules", "cmf-modules","themes", "hooks"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LuYuan",
            "email": "758899293@qq.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Fastcmf\\Modules\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fastcmf\\Modules\\Providers\\ModulesServiceProvider",
                "Fastcmf\\Modules\\Providers\\ThemeServiceProvider"
            ],
            "aliases": {
                "Module": "Fastcmf\\Modules\\Facades\\Module",
                "Theme": "Fastcmf\\Modules\\Facades\\Theme",
                "Hook": "Fastcmf\\Modules\\Facades\\Hook"
            }
        }
    }
}
